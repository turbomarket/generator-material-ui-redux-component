{
  "name": "<%= namelower %>",
  "version": "<%= version %>",
  "description": "React Component + Redux Container <%= use %>",
  "main": "index.jsx",
  "engines": {
    "node": "6.3.0"
  },
  "scripts": {
    "test": "grunt test",
    "lint": "eslint .",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "styleguide-server": "styleguidist server",
    "styleguide-build": "styleguidist build",
    "storybook": "start-storybook -p 9001",
    "fulltest": "npm run flow && npm run lint && grunt test",
    "start": "webpack-dev-server --config webpack.config.js",
    "start-dev": "webpack-dev-server --config webpack.dev.config.js",
    "build": "webpack --config webpack.config.js",
    "build-dev": "webpack --config webpack.dev.config.js",
    "build-doc": "npm run styleguide-build"
  },
  "author": "<%= author %>",
  "license": "<%= license %>",
  "devDependencies": {
    "@kadira/storybook": "^2.2.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-flowtype": "^2.4.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "flow-bin": "^0.30.0",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-contrib-jasmine": "^1.0.3",
    "grunt-webpack": "^1.0.11",
    "jasmine_dom_matchers": "^1.4.0",
    "jsdom": "^9.4.1",
    "react-addons-test-utils": "^15.2.1",
    "react-docgen": "^2.9.1",
    "react-styleguidist": "^3.0.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11"
  },
  "dependencies": {
    "arrayify": "^1.0.0",
    "babel": "^6.5.2",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "material-ui": "^0.15.2",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-query": "0.0.10",
    "react-redux": "^4.4.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
